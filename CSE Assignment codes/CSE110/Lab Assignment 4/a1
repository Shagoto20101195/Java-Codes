public class Classwork
{
  public static void main(String[]args)
  {
    int num = 24;
    int i;
    int sum = 0;
    for(i=0; i<=5; i++)
    {
      if(num>=-6)
      {
        sum = sum + num;
        System.out.println(num);
        num = num - 6;
      }
    }
    System.out.println(sum);
  }
}